<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soyoung.photo.freeanglequeen.dao.StateMapper">

    <!-- 范宏伦 -->
    <!-- 根据 ID 查询国家信息 -->
    <select id="selStateId" resultType="state">
        select id,sName,sUrl from state
          where
        id = #{id}
    </select>

    <!-- 陈海嘉 -->
    <!--//查询所有国家
    public List<State> getStateList();-->
    <select id="getStateList" resultType="State">
        select  * from state
    </select>

</mapper>