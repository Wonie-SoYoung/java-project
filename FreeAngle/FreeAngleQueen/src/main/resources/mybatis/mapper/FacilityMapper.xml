<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soyoung.photo.freeanglequeen.dao.FacilityMapper">

    <!-- 根据 ID 查询设备信息 -->
    <select id="selFacilityID" resultType="facility">
        select id,fName,parentId,minuteId from facility
          where
        id = #{id}
    </select>

</mapper>